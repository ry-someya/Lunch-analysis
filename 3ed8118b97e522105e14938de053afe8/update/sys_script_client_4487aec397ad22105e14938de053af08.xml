        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
  if (isLoading || newValue == oldValue || !newValue) {
    return;
  }

  var userId = g_user.userID;
  if (!userId) return;

  var ga = new GlideAjax('GetSameDateLunchData');
  ga.addParam('sysparm_name', 'recordExists');
  ga.addParam('sysparm_date', newValue); // onChange対象がdateなのでnewValueを使う
  ga.addParam('sysparm_user_id', userId);
  ga.getXMLAnswer(function(response) {
    if (response === 'true') {
      g_form.showFieldMsg('date', 'この日付は既にデータを登録済みです。', 'info');
    }
  });
}

]]></script>
